//Window Start
var favWin = Ti.UI.createWindow({
	title : 'Stored',
	backgroundColor : '#fff'
});
exports.favWin = favWin;
//Window End

var dataB = Ti.Database.open("MarvelDB");
dataB.execute('CREATE TABLE IF NOT EXISTS location (id INTEGER PRIMARY KEY, name TEXT, population TEXT, lng INTEGER, lat INTEGER, dist TEXT, county TEXT)');

var buildRow = function () {
	var data = [];
	var test = dataB.execute("SELECT * FROM location");
	
	while (test.isValidRow()) {
		var name = test.fieldByName('name');
		var st = test.fieldByName('st');
		var county = test.fieldByName('county');
		var country = test.fieldByName('country');
		var latitude = test.fieldByName('latitude');
		var longitude = test.fieldByName('longitude');
		var dist = test.fieldByName('dist');
		var id = test.fieldByName('id');

		data.push({
			name : name,
			st : st,
			county : county,
			country : country,
			latitude : latitude,
			longitude : longitude,
			dist : dist,
			id : id
		});
		test.next();
	};
	return teamData;
};